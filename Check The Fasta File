#!/usr/bin/env python3
from Bio import SeqIO

teste = SeqIO.parse('file.fasta', 'fasta')
teste_list = list(teste)
print(f"Number of sequences: {len(teste_list)}")

for record in teste_list:
    # Check for non-ATGC characters
    if any(char not in 'ATGC' for char in str(record.seq).upper()):
        print(f"Non-canonical nucleotides in: {record.id}")
    else:
        print(f"All nucleotides are canonical in: {record.id}")
    
    # Check length divisible by 3
    if len(record.seq) % 3 != 0:
        print(f"Sequence {record.id} length not divisible by 3: {len(record.seq)}")
    else:
        print(f"Sequence {record.id} length OK: {len(record.seq)}")
